function nextInLine(arr, item) {
    // Only change code below this line
    const tab = arr.push(item);
    const tab2 = arr.shift();
    console.log("wynik: ",arr, " tab: ", tab, "...", tab2);
    return item=tab2;
  
    /*Dodaj liczbę na końcu tablicy, a następnie usuń pierwszy element tablicy.
  
  Funkcja nextInLine powinna następnie zwrócić element, który został usunięty.*/
    // Only change code above this line
  }
  
  // Setup
  const testArr = [9, 2, 3, 4, 5];
  
  // Display code
  console.log("Before: " + JSON.stringify(testArr));
  console.log(nextInLine(testArr, 6));
  console.log("After: " + JSON.stringify(testArr));